<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.veggiechoice.orbital">

    <application
            android:allowBackup="true"
            android:icon="@drawable/pizza_monster"
            android:label="@string/app_name"
            android:roundIcon="@drawable/pizza_monster"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".ProfileDetailActivity">
        </activity>
        <activity android:name=".RestaurantDetailActivity"/>
        <activity android:name=".AddRestaurantActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>